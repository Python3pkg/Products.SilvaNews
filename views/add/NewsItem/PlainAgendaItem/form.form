<?xml version="1.0" encoding="iso-8859-1"?>

<form>
  <title></title>
  <name>form</name>
  <action></action>
  <enctype>None</enctype>
  <method>POST</method>

  <groups>
    <group>
      <title>Default</title>
      <fields>

      <field><id>object_id</id> <type>StringField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <max_length></max_length>
          <external_validator></external_validator>
          <display_maxwidth></display_maxwidth>
          <display_width type="int">20</display_width>
          <required type="int">1</required>
          <css_class></css_class>
          <default></default>
          <title>Id</title>
          <truncate type="int">0</truncate>
          <description></description>
          <extra></extra>
        </values>
        <tales>
        </tales>
      </field>
      <field><id>object_title</id> <type>TextAreaField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <max_linelength></max_linelength>
          <max_length></max_length>
          <max_lines></max_lines>
          <width type="int">40</width>
          <external_validator></external_validator>
          <height type="int">2</height>
          <required type="int">1</required>
          <css_class></css_class>
          <default></default>
          <title>Title</title>
          <description></description>
          <extra></extra>
          <unicode type="int">1</unicode>
        </values>
        <tales>
        </tales>
      </field>
      <field><id>start_datetime</id> <type>DateTimeField</type>
        <values>
          <date_only type="int">0</date_only>
          <alternate_name></alternate_name>
          <input_style>text</input_style>
          <hidden type="int">0</hidden>
          <input_order>dmy</input_order>
          <time_separator>:</time_separator>
          <date_separator>/</date_separator>
          <external_validator></external_validator>
          <required type="int">1</required>
          <default_now type="int">0</default_now>
          <css_class></css_class>
          <title>Startdate/time</title>
          <description>Format: DD/MM/YYYY</description>
        </values>
        <tales>
          <time_separator>python:form.get_time_punctuation()</time_separator>
          <date_separator>python:form.get_date_punctuation()</date_separator>        </tales>
      </field>
      <field><id>subjects</id> <type>MultiCheckBoxField</type>
        <values>
          <alternate_name></alternate_name>
          <description></description>
          <required type="int">1</required>
          <hidden type="int">0</hidden>
          <items>[]</items>
          <default>[]</default>
          <title>Subjects</title>
          <orientation>vertical</orientation>
          <css_class></css_class>
          <external_validator></external_validator>
          <unicode type="int">1</unicode>
        </values>
        <tales>
          <items>python:form.service_news.subject_form_tree()</items>
          <description>python: test(form.service_news.subjects(), 'Choose subjects', '&lt;span style="color: red"&gt;No subjects defined yet! Ask the manager to add subjects to the news service!&lt;/span&gt;')</description>
        </tales>
      </field>
      <field><id>target_audiences</id> <type>MultiCheckBoxField</type>
        <values>
          <alternate_name></alternate_name>
          <description></description>
          <required type="int">1</required>
          <hidden type="int">0</hidden>
          <items>[]</items>
          <default>[]</default>
          <title>Target audiences</title>
          <orientation>vertical</orientation>
          <css_class></css_class>
          <external_validator></external_validator>
          <unicode type="int">1</unicode>
        </values>
        <tales>
          <items>python:form.service_news.target_audience_form_tree()</items>
          <description>python: test(form.service_news.target_audiences(), 'Choose target audiences', '&lt;span style="color: red"&gt;No target audiences defined yet! Ask the manager to add target audiences to the news service!&lt;/span&gt;')</description>
        </tales>
      </field>
      </fields>
    </group>
  </groups>
</form>
