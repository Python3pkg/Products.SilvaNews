<?xml version="1.0" encoding="UTF-8"?>

<form>
  <title></title>
  <name>form</name>
  <action></action>
  <enctype>None</enctype>
  <method>POST</method>

  <i18n_domain>silva_news</i18n_domain>
  <i18n>form</i18n>

  <groups>
    <group>
      <title>Default</title>
      <fields>

      <field><id>object_id</id> <type>StringField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <max_length></max_length>
          <external_validator></external_validator>
          <display_maxwidth></display_maxwidth>
          <display_width type="int">20</display_width>
          <required type="int">1</required>
          <css_class>store</css_class>
          <default></default>
          <title>id</title>
          <truncate type="int">0</truncate>
          <description>No spaces or special characters besides &amp;#8216;_&amp;#8217; or &amp;#8216;-&amp;#8217; or &amp;#8216;.&amp;#8217;</description>
          <extra></extra>
          <unicode type="int">1</unicode>
        </values>
        <tales>
        </tales>
      </field>
      <field><id>object_title</id> <type>StringField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <max_length></max_length>
          <external_validator></external_validator>
          <display_maxwidth></display_maxwidth>
          <display_width type="int">20</display_width>
          <required type="int">1</required>
          <css_class>store</css_class>
          <default></default>
          <title>title</title>
          <description>The title will be publicly visible, and is used as the default link in indexes.</description>
          <extra></extra>
          <unicode type="int">1</unicode>
        </values>
        <tales>
        </tales>
      </field>
      <field><id>start_datetime</id> <type>DateTimeField</type>
        <values>
          <allow_empty_time type="int">1</allow_empty_time>
          <date_only type="int">0</date_only>
          <alternate_name></alternate_name>
          <input_style>text</input_style>
          <hidden type="int">0</hidden>
          <input_order>dmy</input_order>
          <time_separator>:</time_separator>
          <date_separator>/</date_separator>
          <external_validator></external_validator>
          <required type="int">1</required>
          <default_now type="int">0</default_now>
          <css_class></css_class>
          <title>start date/time</title>
          <description>Format: d/m/y, time is optional.</description>
        </values>
        <tales>
          <time_separator>python:form.get_time_punctuation()</time_separator>
          <date_separator>python:form.get_date_punctuation()</date_separator>        </tales>
      </field>
      <field><id>subjects</id> <type>MultiCheckBoxField</type>
        <values>
          <alternate_name></alternate_name>
          <description></description>
          <required type="int">1</required>
          <hidden type="int">0</hidden>
          <items>[]</items>
          <default>[]</default>
          <title>subjects</title>
          <orientation>vertical</orientation>
          <css_class></css_class>
          <external_validator></external_validator>
          <unicode type="int">1</unicode>
        </values>
        <tales>
          <items>python:form.service_news.subject_form_tree()</items>
          <description>python: test(form.service_news.subjects(), 'Choose at least one subject.', '&lt;span style="color: #c00"&gt;No subjects defined yet! Ask the manager to add subjects to the news service!&lt;/span&gt;')</description>
        </tales>
      </field>
      <field><id>target_audiences</id> <type>MultiCheckBoxField</type>
        <values>
          <alternate_name></alternate_name>
          <description></description>
          <required type="int">1</required>
          <hidden type="int">0</hidden>
          <items>[]</items>
          <default>[]</default>
          <title>target audiences</title>
          <orientation>vertical</orientation>
          <css_class></css_class>
          <external_validator></external_validator>
          <unicode type="int">1</unicode>
        </values>
        <tales>
          <items>python:form.service_news.target_audience_form_tree()</items>
          <description>python: test(form.service_news.target_audiences(), 'Choose target audiences.', '&lt;span style="color: #c00"&gt;No target audiences defined yet! Ask the manager to add target audiences to the news service!&lt;/span&gt;')</description>
        </tales>
      </field>
      </fields>
    </group>
  </groups>
</form>
