<?xml version="1.0" encoding="iso-8859-1"?>

<form>
  <title></title>
  <name>add_course_form</name>
  <action></action>
  <enctype>None</enctype>
  <method>POST</method>

  <groups>
    <group>
      <title>Default</title>
      <fields>

      <field><id>object_id</id> <type>StringField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <max_length></max_length>
          <external_validator></external_validator>
          <display_maxwidth></display_maxwidth>
          <display_width type="int">20</display_width>
          <required type="int">1</required>
          <css_class></css_class>
          <default></default>
          <title>id</title>
          <truncate type="int">0</truncate>
          <description>id: no spaces or special characters besides an underscore (_).</description>
          <extra>style="width:100%;"</extra>
        </values>
        <tales>
        </tales>
      </field>
      <field><id>object_title</id> <type>TextAreaField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <max_linelength></max_linelength>
          <max_length></max_length>
          <max_lines></max_lines>
          <width type="int">40</width>
          <external_validator></external_validator>
          <height type="int">2</height>
          <required type="int">1</required>
          <css_class></css_class>
          <default></default>
          <title>Title</title>
          <description>The title will be publicly visible, and is used as the default link in indexes.</description>
          <extra>style="width:100%;"</extra>
        </values>
        <tales>
        </tales>
      </field>
      </fields>
    </group>
  </groups>
</form>
