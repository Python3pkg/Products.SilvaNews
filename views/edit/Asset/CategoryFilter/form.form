<?xml version="1.0"?>

<form>
  <title>uclsubjectaudiencefilter</title>
  <row_length>4</row_length>
  <name>add_form</name>
  <action></action>
  <method>POST</method>
  <enctype></enctype>
  <encoding>UTF-8</encoding>
  <stored_encoding>ISO-8859-1</stored_encoding>
  <unicode_mode>false</unicode_mode>
  <i18n_domain>Silva</i18n_domain>
  <i18n>default_form_add</i18n>
  <groups>
    <group>
      <title>Default</title>
      <fields>   
       <field><id>subjects</id> <type>MultiCheckBoxField</type>
        <values>
          <alternate_name></alternate_name>
          <required type="int">0</required>
          <hidden type="int">0</hidden>
          <items>[]</items>
          <default>[]</default>
          <title>subjects</title>
          <orientation>vertical</orientation>
          <description>
            Select the news subjects to filter on.
          </description>
          <css_class></css_class>
          <external_validator></external_validator>
          <unicode type="int">1</unicode>
        </values>
        <tales>
          <items>python:form.service_news.subject_form_tree()</items>
	  <default>python:form.REQUEST.model.subjects()</default>
        </tales>
      </field>
      <field><id>target_audiences</id> <type>MultiCheckBoxField</type>
        <values>
          <alternate_name></alternate_name>
          <required type="int">0</required>
          <hidden type="int">0</hidden>
          <items>[]</items>
          <default>[]</default>
          <title>target audiences</title>
          <orientation>vertical</orientation>
          <description>
            Select the target audiences to filter on.
          </description>
          <css_class></css_class>
          <external_validator></external_validator>
          <unicode type="int">1</unicode>
        </values>
        <tales>
          <items>python: form.service_news.target_audience_form_tree()</items>
	  <default>python:form.REQUEST.model.target_audiences()</default>
        </tales>
      </field>
      </fields>
    </group>
  </groups>
</form>
