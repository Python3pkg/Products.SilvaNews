<?xml version="1.0" encoding="iso-8859-1"?>

<form>
  <title></title>
  <name>form</name>
  <action></action>
  <enctype>None</enctype>
  <method>POST</method>

  <i18n_domain>silva_news</i18n_domain>
  <i18n>form</i18n>

  <groups>
    <group>
      <title>Default</title>
      <fields>

      <field><id>object_title</id> <type>TextAreaField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <max_linelength></max_linelength>
          <max_length></max_length>
          <max_lines></max_lines>
          <width type="int">40</width>
          <external_validator></external_validator>
          <height type="int">2</height>
          <required type="int">1</required>
          <css_class></css_class>
          <default></default>
          <title>title</title>
          <description></description>
          <extra></extra>
          <unicode type="int">1</unicode>
        </values>
        <tales>
        </tales>
      </field>
      <field><id>display_time</id> <type>CheckBoxField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <css_class></css_class>
          <default type="int">0</default>
          <title>Display time</title>
          <description>
            The time part of start and end date/time will only be displayed
            if this is checked.
          </description>
          <external_validator></external_validator>
          <extra></extra>
        </values>
        <tales>
        </tales>
      </field>
      <field><id>start_datetime</id> <type>DateTimeField</type>
        <values>
          <allow_empty_time type="int">1</allow_empty_time>
          <date_only type="int">0</date_only>
          <alternate_name></alternate_name>
          <input_style>text</input_style>
          <input_order>dmy</input_order>
          <hidden type="int">0</hidden>
          <time_separator>:</time_separator>
          <date_separator>/</date_separator>
          <external_validator></external_validator>
          <required type="int">1</required>
          <default_now type="int">0</default_now>
          <css_class></css_class>
          <title>date and time</title>
          <description>
            Format: d/m/y, time is optional (will default to 00:00)
          </description>
        </values>
        <tales>
          <time_separator>python:form.get_time_punctuation()</time_separator>
          <date_separator>python:form.get_date_punctuation()</date_separator>        </tales>
      </field>
      <field><id>end_datetime</id> <type>DateTimeField</type>
        <values>
          <allow_empty_time type="int">1</allow_empty_time>
          <date_only type="int">0</date_only>
          <alternate_name></alternate_name>
          <input_style>text</input_style>
          <input_order>dmy</input_order>
          <hidden type="int">0</hidden>
          <time_separator>:</time_separator>
          <date_separator>/</date_separator>
          <external_validator></external_validator>
          <required type="int">0</required>
          <default_now type="int">0</default_now>
          <css_class></css_class>
          <title>end date and time</title>
          <description>
            Format: d/m/y, time is optional (will default to 00:00)
          </description>
        </values>
        <tales>
          <time_separator>python:form.get_time_punctuation()</time_separator>
          <date_separator>python:form.get_date_punctuation()</date_separator>        </tales>
      </field>
      <field><id>location</id> <type>TextAreaField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <max_linelength></max_linelength>
          <max_length></max_length>
          <max_lines></max_lines>
          <width type="int">40</width>
          <external_validator></external_validator>
          <height type="int">2</height>
          <required type="int">0</required>
          <css_class></css_class>
          <default></default>
          <title>location</title>
          <description></description>
          <unicode type="int">1</unicode>
          <extra></extra>
        </values>
        <tales>
        </tales>
      </field>
      </fields>
    </group>
  </groups>
</form>
