<?xml version="1.0" encoding="iso-8859-1"?>

<form>
  <title></title>
  <name>tab_metadata_extra_form</name>
  <action></action>
  <enctype></enctype>
  <method>POST</method>

  <groups>
    <group>
      <title>Default</title>
      <fields>

      <field><id>acquire_addables</id> <type>CheckBoxField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <css_class></css_class>
          <default type="int">1</default>
          <title>Acquire addables</title>
          <description>If checked, inherit the allowed addables.</description>
          <external_validator></external_validator>
          <extra></extra>
        </values>
        <tales>
          <default>python:form.get_acquire_addables_checked()</default>
        </tales>
      </field>
      <field><id>addables</id> <type>MultiCheckBoxField</type>
        <values>
          <alternate_name></alternate_name>
          <required type="int">0</required>
          <hidden type="int">0</hidden>
          <items>[]</items>
          <default>[]</default>
          <title>Addables</title>
          <description>If 'acquire addables' is not checked, these settings will determine the items allowed in this publication. Otherwise the addables are acquired from above.</description>
          <external_validator></external_validator>
          <css_class></css_class>
          <orientation>vertical</orientation>
        </values>
        <tales>
          <items>form/REQUEST/model/get_all_addables_allowed_in_newssource</items>
          <default>python: form.get_addables_allowed()</default>
        </tales>
      </field>
      </fields>
    </group>
  </groups>
</form>
