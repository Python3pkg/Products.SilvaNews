<?xml version="1.0" encoding="iso-8859-1"?>

<form>
  <title>Settingsform</title>
  <name>form</name>
  <action></action>
  <enctype>None</enctype>
  <method>POST</method>

  <i18n_domain>silva_news</i18n_domain>
  <i18n>form</i18n>

  <groups>
    <group>
      <title>Default</title>
      <fields>

      <field><id>object_title</id> <type>StringField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <max_length></max_length>
          <external_validator></external_validator>
          <display_maxwidth></display_maxwidth>
          <display_width type="int">20</display_width>
          <required type="int">1</required>
          <css_class></css_class>
          <default></default>
          <title>Title</title>
          <truncate type="int">0</truncate>
          <description></description>
          <extra></extra>
        </values>
        <tales>
          <default>python: form.REQUEST.model.get_title_editable()</default>
        </tales>
      </field>
      <field><id>is_private</id> <type>CheckBoxField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <css_class></css_class>
          <default type="int">0</default>
          <title>Restrict usage</title>
          <description></description>
          <external_validator></external_validator>
          <extra></extra>
        </values>
        <tales>
          <default>python: form.REQUEST.model.is_private()</default>
        </tales>
      </field>
      </fields>
    </group>
  </groups>
</form>