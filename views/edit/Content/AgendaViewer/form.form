<?xml version="1.0" encoding="iso-8859-1"?>

<form>
  <title></title>
  <name>form</name>
  <action></action>
  <enctype>None</enctype>
  <method>POST</method>

  <i18n_domain>silva_news</i18n_domain>
  <i18n>form</i18n>

  <groups>
    <group>
      <title>Default</title>
      <fields>

      <field><id>days_to_show</id> <type>IntegerField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <start></start>
          <external_validator></external_validator>
          <display_maxwidth></display_maxwidth>
          <display_width type="int">20</display_width>
          <required type="int">1</required>
          <css_class></css_class>
          <default></default>
          <title>day range</title>
          <description>Number of days in the future to show.</description>
          <end></end>
          <extra></extra>
        </values>
        <tales>
          <default>python: form.REQUEST.model.days_to_show()</default>
        </tales>
      </field>
      <field><id>number_to_show_archive</id> <type>IntegerField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <start></start>
          <external_validator></external_validator>
          <display_maxwidth></display_maxwidth>
          <display_width type="int">20</display_width>
          <required type="int">1</required>
          <css_class></css_class>
          <default></default>
          <title>number</title>
          <description>number of archive items to show per page</description>
          <end></end>
          <extra></extra>
        </values>
        <tales>
          <default>python: form.REQUEST.model.number_to_show_archive()</default>
        </tales>
      </field>
      <field><id>filters</id> <type>MultiCheckBoxField</type>
        <values>
          <alternate_name></alternate_name>
          <required type="int">1</required>
          <hidden type="int">0</hidden>
          <css_class></css_class>
          <default>[]</default>
          <title>filters</title>
          <description></description>
          <external_validator></external_validator>
          <items>[]</items>
          <orientation>vertical</orientation>
          <unicode type="int">1</unicode>
        </values>
        <tales>
          <items>python: form.REQUEST.model.findfilters_pairs()</items>
          <default>python: form.REQUEST.model.filters()</default>
        </tales>
      </field>
      </fields>
    </group>
  </groups>
</form>
