<?xml version="1.0" encoding="iso-8859-1"?>

<form>
  <title></title>
  <name>form</name>
  <action></action>
  <enctype>None</enctype>
  <method>POST</method>

  <i18n_domain>silva_news</i18n_domain>
  <i18n>form</i18n>

  <groups>
    <group>
      <title>Default</title>
      <fields>
      <field>
        <id>rss_feeds</id> 
        <type>TextAreaField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <max_length></max_length>
          <external_validator></external_validator>
          <display_maxwidth></display_maxwidth>
          <display_width type="int">60</display_width>
          <required type="int">0</required>
          <css_class></css_class>
          <default></default>
          <title>RSS feeds</title>
          <truncate type="int">0</truncate>
          <description>List the URLs of RSS feeds (one per line) to use as sources of news items.</description>
          <extra></extra>
        </values>
        <tales>
          <default>python: '\n'.join(form.REQUEST.model.get_feeds())</default>
        </tales>
      </field>
      </fields>
    </group>
  </groups>
</form>
