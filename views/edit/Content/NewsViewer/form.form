<?xml version="1.0" encoding="iso-8859-1"?>

<form>
  <title></title>
  <name>form</name>
  <action></action>
  <enctype>None</enctype>
  <method>POST</method>

  <groups>
    <group>
      <title>Default</title>
      <fields>

      <field><id>object_title</id> <type>TextAreaField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <max_linelength></max_linelength>
          <max_length></max_length>
          <max_lines></max_lines>
          <width type="int">40</width>
          <external_validator></external_validator>
          <height type="int">2</height>
          <required type="int">1</required>
          <css_class></css_class>
          <default></default>
          <title>Title</title>
          <description></description>
          <extra></extra>
        </values>
        <tales>
        </tales>
      </field>
      <field><id>number_to_show</id> <type>IntegerField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <start></start>
          <external_validator></external_validator>
          <display_maxwidth></display_maxwidth>
          <display_width type="int">20</display_width>
          <required type="int">1</required>
          <css_class></css_class>
          <default></default>
          <title>Number to show</title>
          <description></description>
          <end></end>
          <extra></extra>
        </values>
        <tales>
        </tales>
      </field>
      <field><id>number_is_days</id> <type>RadioField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <css_class></css_class>
          <orientation>vertical</orientation>
          <required type="int">1</required>
          <items>[('Yes', '1'), ('No', '0')]</items>
          <default></default>
          <title>Number is days back</title>
          <first_item type="int">0</first_item>
          <description>When set to 'yes', the number filled in above is the number of days back the filter should return items from. Otherwise the number is the number of items.</description>
          <external_validator></external_validator>
        </values>
        <tales>
          <items>python: [('Yes', 1), ('No', 0)]</items>
        </tales>
      </field>
      <field><id>filters</id> <type>MultiCheckBoxField</type>
        <values>
          <alternate_name></alternate_name>
          <description></description>
          <required type="int">1</required>
          <hidden type="int">0</hidden>
          <css_class></css_class>
          <default>[]</default>
          <title>Filters</title>
          <orientation>vertical</orientation>
          <items>[]</items>
          <external_validator></external_validator>
        </values>
        <tales>
          <items>python: form.REQUEST.model.findfilters_pairs()</items>
        </tales>
      </field>
      </fields>
    </group>
  </groups>
</form>