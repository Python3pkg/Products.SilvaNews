<div class="newsitemlistview"
  xmlns:metal="http://xml.zope.org/namespaces/metal"
  xmlns:tal="http://xml.zope.org/namespaces/tal"
  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  i18n:domain="silva_news">
  <h2 class="heading newsitemheading"
    tal:content="structure content/get_title" />
  <div class="event_actions">
    <a tal:attributes="href view/event_url"
       title="Add this event to your calendar">
      <img tal:attributes="src view/event_img_url" />
      Add this event to my calendar
    </a>
  </div>
  <div class="newsiteminfo"
       tal:condition="view/article_date">
    <p class="p publicationdate">
      <tal:translate i18n:translate="">Publication date:</tal:translate>
      <span class="datetime"
            tal:content="view/article_date">
        display date
      </span>
    </p>
  </div>

  <div class="newsitemcontent"
       tal:content="structure view/article">
    Article
  </div>
</div>
