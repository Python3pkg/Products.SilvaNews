<div class="agenda_item_search_result newsitemlistitem">
  <div class="event_actions">
    <a tal:attributes="href view/event_url"
       title="Add this event to your calendar">
      <img tal:attributes="src view/event_img_url" />
      Add to my calendar
    </a>
  </div>
  <h3 class="heading newsitemheading"
      tal:content="structure content/get_title" />
  <div class="newsiteminfo">
   <tal:infopublic
     tal:condition="view/article_date">
    <p class="p newsiteminfopublicationdate">
     <tal:translate i18n:translate="">Publication: </tal:translate>
     <span class="newsitempublicationdate"
         tal:content="view/article_date" />
    </p>
   </tal:infopublic>

   <p class="p eventdates"
     tal:define="
       startdate view/formatted_start_date | nothing;
       enddate view/formatted_end_date | nothing"
     tal:condition="python: startdate or enddate">
    <tal:if condition="startdate">
     <span tal:omit-tag="" i18n:translate="">Start:</span>
     <tal:startdate replace="startdate" />
    </tal:if>
    <tal:if condition="enddate"><br />
     <span tal:omit-tag="" i18n:translate="">End:</span>
     <tal:enddate replace="enddate" />
    </tal:if>
   </p>
  </div>

  <div class="newsintro">
   <img
     tal:define="classname string:newsitem-thumbnail"
     tal:replace="structure python: content.get_thumbnail(classname)"
    />
   <tal:insertintro
      replace="structure view/article" />
  </div>

  <p class="p articlelink">
   <a href="#" tal:attributes="href context/@@absolute_url" i18n:translate="">
    more ...
   </a>
  </p>
</div>
